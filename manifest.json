{
  "manifest_version": 2,
  "name": "Times Puzzle Keybinds",
  "version": "1.0",
  "description": "Add custom keybindings for Times puzzles.",
  "content_scripts": [
    {
      "matches": ["https://www.thetimes.co.uk/puzzles/sudoku/*"],
      "js": ["scripts/sudokukeybinds.js"]
    },
    {
      "matches": ["https://www.thetimes.co.uk/puzzles/crossword*"],
      "js": ["scripts/crosswordkeybinds.js"]
    } 
  ]
}
